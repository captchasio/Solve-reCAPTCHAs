{
   "action": {
      "default_icon": {
         "128": "assets/images/icon_128.png",
         "32": "assets/images/icon_32.png"
      },
      "default_popup": "popup/popup.html",
      "default_title": "Solve ReCaptchas"
   },
   "background": {
      "service_worker": "service_worker.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content/style.css" ],
